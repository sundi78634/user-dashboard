{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  },
    "proxy": {
      "/api": {
        "target": "http://jsonplaceholder.typicode.com/",
        "changeOrigin": true,
        "pathRewrite": { "^/api" : "" }
      }
    },
    "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ],
      "theme": {
      "@primary-color": "#1DA57A",
      "@link-color": "#1DA57A",
      "@border-radius-base": "2px",
      "@font-size-base": "16px",
      "@line-height-base": "1.2"
    },
    "disableCSSModules": false,
    "publicPath": "/",
    "autoprefixer": null
}
